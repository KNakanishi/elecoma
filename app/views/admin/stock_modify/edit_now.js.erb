ids = <%= @product_styles.map {|ps| ps.id} %>;
<% if @e_id > 0 then %> // error
  error_list[ids.indexOf(<%= @product_style.id %>)] = <%= @e_id %>;
  count_list[ids.indexOf(<%= @product_style.id %>)] = false;
<% elsif @e_id == 0 then  %> // success
  document.getElementById("act"+<%= @product_style.id %>).innerHTML = "<%= number_with_delimiter(@product_style.actual_count) %>";
  success_list[ids.indexOf(<%= @product_style.id %>)] = true;
<%end%>
ajax_list[ids.indexOf(<%= @product_style.id %>)] = false;
show_sync(ids);
